<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox alignment="CENTER_RIGHT" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" style="-fx-background-color: #3c3f41;" stylesheets="@/css/menue.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.MenueController">
    <children>
      <Label text="%test.language" textFill="WHITE" />
        <ToggleButton fx:id="tbtEn" mnemonicParsing="false" onAction="#setLanguageToEnglish" prefHeight="26.0" prefWidth="45.0" text="EN">
            <HBox.margin>
                <Insets left="5.0" right="5.0" />
            </HBox.margin>
        </ToggleButton>
        <ToggleButton fx:id="tbtDe" mnemonicParsing="false" onAction="#setLanguageToGerman" prefHeight="26.0" prefWidth="45.0" text="DE">
            <HBox.margin>
                <Insets left="5.0" right="5.0" />
            </HBox.margin>
        </ToggleButton>
        <Region prefHeight="200.0" prefWidth="200.0" />
        <Button fx:id="btLogout" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#handleLogout" prefHeight="26.0" prefWidth="76.0" text="Logout">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
        </Button>
    </children>
</HBox>
